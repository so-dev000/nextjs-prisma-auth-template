{
  "name": "nextjs-prisma-auth-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "setup": "cp .env.example .env && npm run db:migrate",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:generate": "prisma generate",
    "db:generate:sql": "prisma generate --sql",
    "db:generate:watch": "prisma generate --sql --watch",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mantine/core": "^8.3.5",
    "@mantine/dates": "^8.3.5",
    "@mantine/form": "^8.3.5",
    "@mantine/hooks": "^8.3.5",
    "@prisma/client": "^6.17.1",
    "dayjs": "^1.11.18",
    "next": "^15.5.6",
    "next-auth": "^5.0.0-beta.29",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "prisma": "^6.17.1",
    "typescript": "^5"
  }
}
